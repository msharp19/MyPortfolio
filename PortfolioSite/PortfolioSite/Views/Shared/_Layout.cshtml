<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Matt Sharps Portfolio</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/style.css")
    @Styles.Render("~/Content/css/bootstrap.min.css")
    @Styles.Render("~/Content/css/responsive.css")
    @Styles.Render("~/Content/css/font-awesome.min.css")
    @Styles.Render("~/Content/css/set2.css")
    @Styles.Render("~/Content/css/normalize.css")
    @Styles.Render("~/Content/css/component.css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="shortcut icon" href="../images/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <header role="header">
        <div class="container">
            <h1>
                <a href="../Home/Index" title="Matt Sharp"><img src="../images/my-logo-1.jpg" class="logo-size" title="Matt Sharp" alt="Matt Sharp" /></a>
            </h1>
            <nav role="header-nav" class="navy">
                <ul>
                    <li class="nav-active">@Html.ActionLink("Work", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Blog", "Index", "Blog")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Contact")</li>
                </ul>
            </nav>
        </div>
    </header>
    <a href="javascript:" id="return-to-top" style="display: none;"><i class="fa fa-chevron-up"></i></a>
    @RenderBody()
    <footer role="footer">
        <h1>
            <a href="../Home/Index" class="logo-size" title="Matt Sharp"><img src="../images/my-logo-1.jpg" class="logo-size" title="Matt Sharp" alt="Matt Sharp" /></a>
        </h1>
        <nav role="footer-nav">
            <ul>
                <li>@Html.ActionLink("Work", "Index", "Home")</li>
                <li>@Html.ActionLink("About", "About", "Home")</li>
                <li>@Html.ActionLink("Blog", "Index", "Blog")</li>
                <li>@Html.ActionLink("Contact", "Contact", "Contact")</li>
            </ul>
        </nav>
        <ul role="social-icons">
            <li><a href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
            <li><a href="#"><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
            <li><a href="#"><i class="fa fa-youtube" aria-hidden="true"></i></a></li>
        </ul>
        <p class="copy-right">&copy; @DateTime.Now.Year - Matt Sharps Portfolio</p>
        <p class="copy-right">&copy; 2015  avana LLC.. All rights Reserved</p>

    </footer>

   <!--Old here-->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/Scripts/js/html5shiv.js")
    @Scripts.Render("~/Scripts/js/modernizr.custom.js")
    @Scripts.Render("~/Scripts/js/AnimOnScroll.js")
    @Scripts.Render("~/Scripts/js/classie.js")
    @Scripts.Render("~/Scripts/js/masonry.pkgd.min.js")
    @Scripts.Render("~/Scripts/js/imagesloaded.js")
    @Scripts.Render("~/Scripts/js/bootstrap.min.js")
    @Scripts.Render("~/Scripts/js/custom.js")
    @Scripts.Render("~/Scripts/js/nav.js")
    @Scripts.Render("~/Scripts/js/jquery.min.js")
    @Scripts.Render("~/Scripts/js/layout.js")
    @Scripts.Render("~/Scripts/layout.js")
    @RenderSection("scripts", required: false)
</body>
</html>
