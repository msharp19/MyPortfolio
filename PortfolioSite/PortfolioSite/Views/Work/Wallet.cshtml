
@{
    ViewBag.Title = "WorkItem";
}
<main role="main-inner-wrapper" class="container">
    <div class="work-details">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-4">
                <header role="work-title">
                    <h2>MO-ETH Wallet</h2>
                    <a href="#">Project URL: Private <i class="fa fa-lock" aria-hidden="true"></i></a>
                    <!--<i class="fa fa-external-link" aria-hidden="true"></i>-->
                </header>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-8">
                <section>
                    <p>This is a WinForm Desktop application where users can manage/swap between Morarity (My ICO) & Ethereum and make transactions.</p>
                    <!--<p></p>-->
                    <p>
                        <strong>Details</strong><br />
                        Winform C#
                        .NET Framework 4.5.2
                    </p>
                </section>
            </div>
        </div>
        <div class="clearfix"></div>
        <div class="work-images grid">
            <ul class="grid-lod effect-2" id="grid">
                <li>
                    <a href="~/images/wallet/vs-frame.png" class="pointer">
                        <img src="~/images/wallet/vs-frame.png" alt="" class="img-responsive thumbnail" />
                    </a>
                </li>
                <li>
                    <a href="~/images/wallet/swap-coins.png" class="pointer">
                        <img src="~/images/wallet/swap-coins.png" alt="" class="img-responsive thumbnail" />
                    </a>
                </li>
                <li>
                    <a href="~/images/wallet/login-details.png" class="pointer">
                        <img src="~/images/wallet/login-details.png" alt="" class="img-responsive thumbnail" />
                    </a>
                </li>
                <li>
                    <a href="~/images/wallet/transactions.png" class="pointer">
                        <img src="~/images/wallet/transactions.png" alt="" class="img-responsive thumbnail" />
                    </a>
                </li>
                <li>
                    <a href="~/images/wallet/bank.png" class="pointer">
                        <img src="~/images/wallet/bank.png" alt="" class="img-responsive thumbnail" />
                    </a>
                </li>
            </ul>
        </div>
    </div>
</main>
@Styles.Render("~/Content/css/style.css")